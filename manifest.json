{
  "name": "Grailed Helper",
  "version": "0.1.2",
  "manifest_version": 2,
  "description": "Adds saved filters to grailed.com",
  "permissions": [ "*://*.grailed.com/*", "storage"],
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "content_scripts": [{
    "js":       [  "jquery.min.js", "gh.js" ],
    "matches":  ["*://*.grailed.com/*"]
  }] 
}
